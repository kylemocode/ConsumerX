{
  "compilerOptions": {
    "target": "es2015",  
    "module": "commonjs",
    "declaration": true,
    "outDir": "dist",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
  },
  "include": ["src/**/*"],
  "exclude": [
    "node_modules",
    "dist",
    "src/test/*"
  ]
}
